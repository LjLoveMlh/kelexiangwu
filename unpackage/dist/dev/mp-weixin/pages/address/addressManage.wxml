<view class="content"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['addressData']],['setValue',['name','$0'],['addressData.name']]]]]}}" value="{{addressData.name}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['addressData']],['setValue',['mobile','$0'],['addressData.mobile']]]]]}}" value="{{addressData.mobile}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">地址</text><view class="input flex justify-between padding-right"><view><input class="input" type="text" disabled="true" placeholder="收货人地址" data-event-opts="{{[['blur',[['setValue',['address','$0'],['temp_address']]]]]}}" value="{{temp_address}}" bindblur="__e"/></view><view data-event-opts="{{[['tap',[['chooseLocation',['$event']]]]]}}" class="yticon icon-shouhuodizhi" bindtap="__e"></view></view></view><view class="row b-b"><text class="tit">门牌号</text><input class="input" type="text" placeholder="楼号、门牌" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','area','$event',[]],['addressData']],['setValue',['area','$0'],['addressData.area']]]]]}}" value="{{addressData.area}}" bindinput="__e"/></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button></view>